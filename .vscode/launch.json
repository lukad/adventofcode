  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'solutions'",
      "cargo": {
        "args": [
          "build",
          "--bin=solutions",
          "--package=solutions"
        ],
        "filter": {
          "name": "solutions",
          "kind": "bin"
        }
      },
      "args": [
        "-y2022",
        "-d19",
        "-p1",
        "day19.txt"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'solutions'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=solutions",
          "--package=solutions"
        ],
        "filter": {
          "name": "solutions",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'aoc'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=aoc"
        ],
        "filter": {
          "name": "aoc",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
